{
  "apps": [
    {
      "name": "global-market-tracker",
      "script": "app.py",
      "interpreter": "python3",
      "cwd": "/home/user/webapp",
      "env": {
        "PORT": "8000",
        "PYTHONPATH": "/home/user/webapp"
      },
      "instances": 1,
      "exec_mode": "fork",
      "watch": false,
      "autorestart": true,
      "max_restarts": 10,
      "min_uptime": "10s",
      "log_file": "./logs/app.log",
      "error_file": "./logs/error.log",
      "out_file": "./logs/out.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss Z"
    }
  ]
}